package ch23;

import java.util.ArrayList;

import ch21.Book;

public class ArrayListTest {

	public static void main(String[] args) {
		
		//ArrayList<> 제네릭을 빼도 되긴하지만 써서 사용하는것을 권고한다.
		ArrayList<Book> library = new ArrayList<Book>();
		
		library.add(new Book("태백산맥1", "조정래"));
		library.add(new Book("태백산맥1", "조정래"));
		library.add(new Book("태백산맥1", "조정래"));
		library.add(new Book("태백산맥1", "조정래"));
		library.add(new Book("태백산맥1", "조정래"));
	}

}
