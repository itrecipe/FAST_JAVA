package ch16;

public class SwitchCaseTest2 {
	//문자열이 지원되는 switch ~ case문
	public static void main(String[] args) {
		
		String medal = "Gold";
		
		switch(medal) {
			case "Gold":
				System.out.println("금메달");
				break;
			case "Silver":
				System.out.println("은메달");
				break;
			case "Bronze":
				System.out.println("동메달");
				break;
		}
	}

}
